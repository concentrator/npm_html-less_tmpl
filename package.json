{
  "name": "npm_html-less",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:less": "less-watch-compiler",
    "dev-server": "live-server --ignore=node_modules,less,*.json,*.md ./",
    "start": "npm-run-all --parallel dev-server watch:less",
    "compile:less": "lessc ./less/style.less ./css/style.compiled.css",
    "prefix:css": "postcss ./css/style.compiled.css --use autoprefixer -o ./css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concentrator/npm_html-less_tmpl.git"
  },
  "author": "Dmitry Iovlev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/concentrator/npm_html-less_tmpl/issues"
  },
  "homepage": "https://github.com/concentrator/npm_html-less_tmpl#readme",
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "less": "^3.9.0",
    "less-watch-compiler": "^1.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
